{
    "name": "anchor/docs-coverage-analyzer",
    "description": "Documentation coverage analyzer for PHP projects",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Anchor\\DocsCoverage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Anchor\\DocsCoverage\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "symfony/console": "^6.0",
        "symfony/finder": "^6.0",
        "symfony/yaml": "^6.0",
        "league/commonmark": "^2.0",
        "nikic/php-parser": "^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "phpstan/phpstan": "^1.0"
    },
    "bin": [
        "bin/anchor-docs"
    ]
} 